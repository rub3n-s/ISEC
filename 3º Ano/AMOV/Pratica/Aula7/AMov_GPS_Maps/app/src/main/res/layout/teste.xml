<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#6E5D5D"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.469" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:background="#6E5D5D"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="58dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:background="#6E5D5D"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="155dp"
        android:layout_height="14dp"
        android:background="#6E5D5D"
        android:text="TextView"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="127dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:background="#6E5D5D"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textViewBase"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#6E5D5D"
        android:text="textViewBase"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.161"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.191" />

    <TextView
        android:id="@+id/textViewBase2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:background="#6E5D5D"
        android:text="textViewBase"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintStart_toEndOf="@+id/barrier3"
        tools:layout_editor_absoluteY="539dp" />

    <TextView
        android:id="@+id/textViewBase6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:background="#6E5D5D"
        android:text="textViewBase"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@+id/textViewBase3"
        app:layout_constraintTop_toBottomOf="@+id/textViewBase3" />

    <TextView
        android:id="@+id/textViewBase7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#6E5D5D"
        android:text="textViewBase"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/textViewBase6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textViewBase2" />

    <TextView
        android:id="@+id/textViewBase3"
        android:layout_width="84dp"
        android:layout_height="16dp"
        android:layout_marginTop="44dp"
        android:background="#6E5D5D"
        android:text="textViewBase"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@+id/textViewBase2"
        app:layout_constraintTop_toTopOf="@+id/textViewBase2" />

    <TextView
        android:id="@+id/textViewC1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#6E5D5D"
        android:text="TextViewC1"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/textViewC2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textViewC2" />

    <TextView
        android:id="@+id/textViewC2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:background="#6E5D5D"
        android:text="TextViewC2"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/TextViewC3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewC1"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/TextViewC3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#6E5D5D"
        android:text="TextViewC3"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewC2"
        app:layout_constraintTop_toTopOf="@+id/textViewC2" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        app:constraint_referenced_ids="textViewBase7" />

</androidx.constraintlayout.widget.ConstraintLayout>